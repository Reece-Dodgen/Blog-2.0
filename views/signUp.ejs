                <form action="/signUp" method="post">
                    <h2 class="text-center">
                        <strong>Create</strong>
                        "an account."
                    </h2>
                    <div class="form-group">
                        <input class="form-control" type="email" name="email" placeholder="Email">
                    </div>
                    <div class="form-group">
                        <input class="form-control" type="text" name="user" placeholder="User Name">
                    </div>
                    <div class="form-group">
                        <input class="form-control" type="password" name="password" placeholder="Password">
                    </div>
                    <div class="form-group">
                        <input class="form-control" type="password" name="password-repeat" placeholder="Password (repeat)">
                    </div>
                    <div class="form-group">
                        <div class="checkbox">
                            <label class="control-lable">
                                <input type="checkbox" /> "I agree to the license terms."
                            </label>
                        </div>
                    </div>
                    <div class="form-group">
                        <button class="btn-left" class="btn btn-primary btn-block" type="submit">Sign Up</button>
                    </div>
